!!!
%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}/
    %meta{:content => "width=device-width, initial-scale=1, shrink-to-fit=no", :name => "viewport"}/
    %meta{:content => "ie=edge", "http-equiv" => "x-ua-compatible"}/
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/

    %meta{:content => "1667612320233052", :property => "og:app_id"}/
    %meta{:content => "http://www.raheem.ai/assets/facebook-share.png", :property => "og:image"}/
    %meta{:content => "Meet Raheem: a Chatbot that Tracks Police Performance", :property => "og:title"}/
    %meta{:content => "Raheem is a Facebook Messenger bot designed to listen to your stories about police interactions & build an open database on police performance.", :property => "og:description"}/
    %meta{:content => "Meet Raheem", :property => "og:site_name"}/
    %meta{:content => "www.seed-web.herokuapp.com", :property => "og:url"}/
    %meta{:content => "hello@raheem.ai", :property => "og:email"}/
    %meta{:content => "article", :property => "og:type"}/
    





    %title Seed
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
  %body
    = render 'shared/nav'
    = render 'shared/flash'
    
    = yield
